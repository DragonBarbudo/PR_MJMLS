.carousel.slide(data-ride="carousel")#mainbanner
  .carousel-inner
    .carousel-item.active
      img(src="http://placehold.it/1200x600").d-block.w-100
    .carousel-item
      img(src="http://placehold.it/1200x601").d-block.w-100
  .carousel-control-prev(href="#mainbanner" role="button" data-slide="prev")
    span.carousel-control-prev-icon
  .carousel-control-next(href="#mainbanner" role="button" data-slide="next")
    span.carousel-control-next-icon